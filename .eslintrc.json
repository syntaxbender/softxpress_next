{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-curly-brace-presence": ["error", { "props": "never", "children": "never" }],
        "no-undef": "error",
    "no-unused-vars": "warn"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
}
